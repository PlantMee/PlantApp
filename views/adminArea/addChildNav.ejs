<%- include("../partials/adminHeader"); -%>

<div class="container" style="width: 35%;margin-top: 8%;border: 1px solid #dddddd;padding-top: 2%;">
	<form  action="/admin/childNav" method="POST">
	  <div class="form-row align-items-center">
		  <div class="">
    		  <label class="sr-only" for="inlineFormInputGroupUsername">Title</label>
   		  	 <div class="input-group mb-3">
				<div class="input-group-prepend">
         			<div class="input-group-text">Title </div>
        		</div>
       		 	<input type="text" class="form-control" name="title" id="inlineFormInputGroupUsername"placeholder="PAGE TITLE" required>
     		 </div>
  		  </div>

			
			<div class="input-group mb-3">
		  		<div class="input-group-prepend">
		   			 <label class="input-group-text" for="inputGroupSelect01">ParentNav</label>
		 	   </div>
			  <select name="parentTag" class="custom-select" id="inputGroupSelect01">			    
			    <% parentNav.forEach(function (items){  %>
			    <option value="<%= items._id %>"><%= items.text  %></option>
			    <% }) %>			    
			  </select>
			</div>
			<div class="form-group">
				<div class="text-center">
					<button class="btn btn-primary">ADD ITEM</button>
				</div>	
			</div>
	  </div>
	</form>
</div>




<%- include("../partials/adminFooter"); -%>